{
	"compilerOptions": {
		"strict": true,
		"jsx": "preserve",
		"target": "esnext",
		"module": "esnext",
		"sourceMap": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": [
			"esnext",
			"dom"
		],
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"useDefineForClassFields": true,
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"src/*"
			]
		},
		"types": [
			"vite-plugin-use-modules/client",
			"@intlify/unplugin-vue-i18n/messages",
			"vite-plugin-vue-meta-layouts/client",
			"node"
		]
	},
	"include": [
		"srcipts",
		"presets",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"./vite.config.ts"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}